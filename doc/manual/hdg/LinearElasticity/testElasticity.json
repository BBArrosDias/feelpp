{
    "Name": "HDG-Mixed-Linear-Elasticity",
    "ShortName":"ME",
    "Model":"hdg",
    "Materials":
    {
        "omega":
        {
            "name":"copper",
            "lambda":"1",
	    	"mu":"1"
        }
    },
    "BoundaryConditions":
    {
        "strain":
        {
            "SourceTerm":
            {
                "omega":
                {
                   "expr":"{-3*sin(pi*x)*cos(pi*y),-3*sin(pi*y)*cos(pi*x)}:x:y"
                }
            }/*,
            "Neumann":
            {
                "bottom":
                {
                     "expr":"0.0"
                },
                "top":
                {
                     "expr":"0.0"
                }
            }*/
		},
		"displacement":
		{
	    	"Dirichlet":
	    	{
        		"left":
                {
		     		"expr":"{1/(2*pi*pi)*sin(pi*x)*cos(pi*y),1/(2*pi*pi)*cos(pi*x)*sin(pi*y)}:x:y" 
                },
        		"top":
                {
		     		"expr":"{1/(2*pi*pi)*sin(pi*x)*cos(pi*y),1/(2*pi*pi)*cos(pi*x)*sin(pi*y)}:x:y" 
                },
        		"bottom":
                {
		     		"expr":"{1/(2*pi*pi)*sin(pi*x)*cos(pi*y),1/(2*pi*pi)*cos(pi*x)*sin(pi*y)}:x:y" 
                },
				"right":
				{
		     		"expr":"{1/(2*pi*pi)*sin(pi*x)*cos(pi*y),1/(2*pi*pi)*cos(pi*x)*sin(pi*y)}:x:y" 
				}
 	    	}
    	},
		"ExactSolution":
		{
			"u_exact":
			{
				"omega":
				{
		     		"expr":"{1/(2*Pi*Pi)*sin(Pi*x)*cos(Pi*y),1/(2*Pi*Pi)*cos(Pi*x)*sin(Pi*y)}:x:y" 
				}
			}
		}
     },
    "PostProcess":
    {
        "Fields":["strain","displacement"]
    }
}




