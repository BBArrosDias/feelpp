{
    "Name": "HDG-Electro-Thermo ",
    "ShortName":"ET",
    "Model":"hdg",
    "Parameters":
    {
        "Tw":"20",
        "alpha":"0",
        "Lorentz":"2.44e-8"
    },
    "Materials":
    {
        "Omega":
        {
            "name":"copper",
            "sigma0":"48e+6",
            "sigma":"sigma0/(1+alpha*(T-Tw)):sigma0:alpha:T:Tw",
            "k":"sigma*Lorentz*T:sigma:Lorentz:T"
        }
    },
    "BoundaryConditions":
    {
        "potential":
        {
            "Dirichlet":
            {
                "V0":
                {
                    "expr":"1"
                },
                "V1":
                {
                    "expr":"0"
                }
            },
            "Neumann":
            {
                "Channel0":
                {
                    "expr":"0"
                },
                "Channel1":
                {
                    "expr":"0"
                },
                "CoolingHoles":
                {
                    "expr":"0"
                },
                "TieRods":
                {
                    "expr":"0"
                }
            }
        },
        "current":
        {
            "Dirichlet":
            {
                "V1":
                {
                    "expr":"3333"
                }
            }
        },
        "temperature":
        {
            "Robin":
            {
                "Channel0":
                {
                    "expr1":"0.01",
                    "expr2":"0.01*Tw:Tw"
                },
                "Channel1":
                {
                    "expr1":"0.01",
                    "expr2":"0.01*Tw:Tw"
                },
                "TieRods":
                {
                    "expr1":"0.055",
                    "expr2":"0.055*Tw::Tw"
                },
                "CoolingHoles":
                {
                    "expr1":"0.055",
                    "expr2":"0.055*Tw:Tw"
                }
            }
        }
    },
    "PostProcess":
    {
        "Fields":["potential","current","temperature"]
    }    
