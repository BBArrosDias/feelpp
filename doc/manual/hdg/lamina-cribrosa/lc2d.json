{
    "Name": "HDG-Mixed-Poisson for lamina cribrosa",
    "ShortName":"MP_LC",
    "Model":"hdg",

    "Materials":
    {
        "omega":
        {
            "name":"copper",
            "cond":"-1",
	    "R_ext":"0.095",
	    "k":"10",
	    "H":"0.03",
	    "CC":"1",
	    "RR":"1" 
        }
    },
    "BoundaryConditions":
    {
        "potential":
        {
            "SourceTerm":
            {
                "omega":
                {
                    "expr":"4*k*( x*x * sin(x*x+y*y) - cos(x*x+y*y) + y*y * sin(x*x + y*y) ):x:y:k:t" // "-sin(Pi*x)*sin(Pi*y):x:y"
                }
            },
            "Dirichlet":
	    {
	    	"top":
        	{
                     "expr":"2 + sin(x*x+y*y) + 16*Pi*R_ext*R_ext :k:x:y:R_ext"   // "sin(Pi*x)/(2*Pi):x"
               	}
	    }
	},
	"flux":
	{
	    "Integral":
	    {
        	"bottom":
                {
                     "expr":"-4*Pi*R_ext*R_ext*k*cos(R_ext*R_ext):k:R_ext"  // "-sin(Pi*x)/(2*Pi):x"
                }
            }
        }
    },
    "PostProcess":
    {
        "Fields":["potential","flux","state variable"]
    }
}




