{
    "Name": "HDG-Mixed-Linear-Elasticity",
    "ShortName":"ME",
    "Model":"hdg",
    "Materials":
    {
        "omega1":
        {
            "name":"copper",
			"rho":"1",
            "lambda":"1",
	    	"mu":"1"
        },
        "omega2":
        {
            "name":"copper",
			"rho":"1",
            "lambda":"1",
	    	"mu":"1"
        }
    },
    "BoundaryConditions":
    {
        "stress":
        {
            "SourceTerm":
            {
                "omega1":
                {
					"expr":"{8,0,0}"
                },
                "omega2":
                {
					"expr":"{8,0,0}"
                }
            },
            "Neumann":
            {
                "base2":
                {
                     "expr":"{6*x+1,2*y,0, 2*y, 2*x+1,0, 0,0,2*x+3}:x:y"
                }
            }
		},
		"displacement":
		{
	    	"Dirichlet":
	    	{
        		"dirichlet1":
                {
		     		"expr":"{x^2+y^2,1,z}:x:y:z" 
                },
        		"lateral2":
                {
		     		"expr":"{x^2+y^2,1,z}:x:y:z" 
                }/*,
        		"interface":
                {
		     		"expr":"{x^2+y^2,1,z}:x:y:z" 
                }*/
 	    	}
    	},
		"ExactSolution":
		{
			"u_exact":
			{
				"omega1":
				{
		     		"expr":"{x^2+y^2,1,z}:x:y:z" 
				},
				"omega2":
				{
		     		"expr":"{x^2+y^2,1,z}:x:y:z" 
				}
            }
		}
     },
    "PostProcess":
    {
        "Fields":["stress","displacement"]
    }
}




