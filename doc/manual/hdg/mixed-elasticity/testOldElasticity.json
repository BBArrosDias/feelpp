{
    "Name": "HDG-Mixed-Linear-Elasticity",
    "ShortName":"ME",
    "Model":"hdg",
    "Materials":
    {
        "omega":
        {
            "name":"copper",
			"rho":"1",
            "lambda":"1",
	    	"mu":"1"
        }
    },
    "BoundaryConditions":
    {
        "stress":
        {
            "SourceTerm":
            {
                "omega":
                {
					"expr":"{8,0,0}"// "{8,0,0}"
                }
            }/*,
            "Neumann":
            {
                "bottom_internal":
                {
                     "expr":"{6*x+1,2*y,0, 2*y, 2*x+1,0, 0,0,2*x+3}:x:y"
                }
            }*/
		},
		"displacement":
		{
	    	"Dirichlet":
	    	{
        		"top":
                {
		     		"expr":"{x^2+y^2,1,z}:x:y:z" 
                },
        		"bottom":
                {
		     		"expr":"{x^2+y^2,1,z}:x:y:z" 
                },
        		"out":
                {
		     		"expr":"{x^2+y^2,1,z}:x:y:z" 
                },
        		"in":
                {
		     		"expr":"{x^2+y^2,1,z}:x:y:z" 
                }
 	    	}
    	},
		"ExactSolution":
		{
			"u_exact":
			{
				"omega":
				{
		     		"expr":"{x^2+y^2,1,z}:x:y:z" 
				}
			}
		}
     },
    "PostProcess":
    {
        "Fields":["stress","displacement"]
    }
}




