{
    "Name": "HDG-Mixed-Linear-Elasticity",
    "ShortName":"ME",
    "Model":"hdg",
    "Materials":
    {
        "cylinder":
        {
            "name":"steel",
            "lambda":"1",
	    	"mu":"1"
        }
    },
    "BoundaryConditions":
    {
        "stress":
        {
            "SourceTerm":
            {
                "cylinder":
                {
					"expr":"{0,0,0}"
                }
            },
            "Integral":
            {
                "top":
                {
                    "expr":"{0,0,0}:x:y:z"
                }
            },
            "Neumann":
            {
                "lateral":
                {
                    "expr":"{0,0,0}:x:y:z"
                }
            }
		},
		"displacement":
		{
	    	"Dirichlet":
	    	{
        		"bottom":
                {
		     		"expr":"{1,0,0}:x:y:z" 
                }
 	    	}
    	},
		"ExactSolution":
		{
			"u_exact":
			{
				"cylinder":
				{
		     		"expr":"{1,0,0}:x:y:z" 
				}
			}
		}
     },
    "PostProcess":
    {
        "Fields":["stress","displacement"]
    }
}




