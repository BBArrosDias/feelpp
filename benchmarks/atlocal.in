# -*- shell-script -*-
# @configure_input@
# Configurable variable values for Autoconf test suite.
#

# stuff that could be needed by the testsuite
with_python=@WITH_PYTHON@
with_python_module_scipy=@WITH_PYTHON_MODULE_SCIPY@
with_python_module_pyx=@WITH_PYTHON_MODULE_PYX@

with_python_support=0
if test "x$with_python"=x1 -a "x$with_python_module_scipy"=x1 -a "x$with_python_module_pyx"=x1; then
with_python_support=1
fi
