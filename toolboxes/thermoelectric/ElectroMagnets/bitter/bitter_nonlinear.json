// -*- mode: javascript -*-
{
    "Name":"bitter",
    "ShortName":"bitter",
    "Parameters":
    {
        "sigma0":58e3,
        "T0":290,
        "alpha":3.6e-3,
        "Lorentz":2.47e-8
    },
    "Materials":
    {
        "omega":
        {
            "k":"sigma0*Lorentz*heat_T/(1+alpha*(heat_T-T0)):sigma0:alpha:T0:Lorentz:heat_T",
            "sigma":"sigma0/(1+alpha*(heat_T-T0))+0*heat_T:sigma0:alpha:T0:heat_T"
        }
    },
    "BoundaryConditions":
    {
        "temperature":
        {
            "Robin":
            {
                "Channel0":
                {
                    "expr1":"0.08",
                    "expr2":"293"
                },
                "Channel1":
                {
                    "expr1":"0.08",
                    "expr2":"293"
                },
                "TieRods":
                {
                    "expr1":"0.08",
                    "expr2":"293"
                },
                "CoolingHoles":
                {
                    "expr1":"0.08",
                    "expr2":"293"
                }
            },
            "Neumann":
            {
                "V0":
                {
                    "expr":"0"
                },
                "V1":
                {
                    "expr":"0"
                },
                "top":
                {
                    "expr":"0"
                },
                "bottom":
                {
                    "expr":"0"
                }
            }
        },
        "electric-potential":
        {
            "Dirichlet":
            {
                "V1":
                {
                    "expr":"-0.04"
                },
                "V0":
                {
                    "expr":"0"
                }
            },
            "Neumann":
            {
                "Channel0":
                {
                    "expr":"0"
                },
                "Channel1":
                {
                    "expr":"0"
                },
                "CoolingHoles":
                {
                    "expr":"0"
                },
                "TieRods":
                {
                    "expr":"0"
                },
                "top":
                {
                    "expr":"0"
                },
                "bottom":
                {
                    "expr":"0"
                }
            }
        }
    },
    "PostProcess":
    {
        "use-model-name":1,
        "thermo-electric":
        {
            "Exports":
            {
                "fields":["heat.temperature","electric.electric-potential","electric.current-density","electric.electric-field","heat.pid"]
            }
        },
        "heat":
        {
            "Measures":
            {
                "Statistics":
                {
                    "FluxV0":
                    {
                        "type":"mean",
                        "expr":"-heat_omega_k*heat_dn_T:heat_omega_k:heat_dn_T",
                        "markers":"Channel0"
                    },
                    "FluxContV0":
                    {
                        "type":"mean",
                        "expr":"0.08*(heat_T-293):heat_T",
                        "markers":"Channel0"
                    },
                    "FluxV1":
                    {
                        "type":"mean",
                        "expr":"-heat_omega_k*heat_dn_T:heat_omega_k:heat_dn_T",
                        "markers":"Channel1"
                    },
                    "FluxContV1":
                    {
                        "type":"mean",
                        "expr":"0.08*(heat_T-293):heat_T",
                        "markers":"Channel1"
                    },
                    "FluxCoolingHoles":
                    {
                        "type":"mean",
                        "expr":"-heat_omega_k*heat_dn_T:heat_omega_k:heat_dn_T",
                        "markers":"CoolingHoles"
                    },
                    "FluxContCoolingHoles":
                    {
                        "type":"mean",
                        "expr":"0.08*(heat_T-293):heat_T",
                        "markers":"CoolingHoles"
                    },
                    "FluxTieRods":
                    {
                        "type":"mean",
                        "expr":"-heat_omega_k*heat_dn_T:heat_omega_k:heat_dn_T",
                        "markers":"TieRods"
                    },
                    "FluxContTieRods":
                    {
                        "type":"mean",
                        "expr":"0.08*(heat_T-293):heat_T",
                        "markers":"TieRods"
                    },
                    "temp":
                    {
                        "type":["max","mean","min"],
                        "field":"temperature"
                    }
                }
            }
        },
        "electric":
        {
            "Measures":
            {
                "Statistics":
                {
                    "Intensity0":
                    {
                        "type":"integrate",
                        "expr":"-electric_omega_sigma*electric_dnP:electric_omega_sigma:electric_dnP",
                        "markers":"V0"
                    },
                    "Intensity1":
                    {
                        "type":"integrate",
                        "expr":"-electric_omega_sigma*electric_dnP:electric_omega_sigma:electric_dnP",
                        "markers":"V1"
                    },
                    "Power":
                    {
                        "type":"integrate",
                        "expr":"electric_omega_sigma*(electric_dxP^2+electric_dyP^2+electric_dzP^2):electric_omega_sigma:electric_dxP:electric_dyP:electric_dzP",
                        "markers":"omega"
                    }
                }
            }
        }
    }
}
