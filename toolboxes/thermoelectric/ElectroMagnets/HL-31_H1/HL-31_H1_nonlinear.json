// -*- mode: javascript -*-
{
    "Name":"HL-31_H1",
    "ShortName":"HL-31_H1",
    "Parameters":
    {
        "sigma0":53e3,
        "T0":290,
        "alpha":3.6e-3,
        "Lorentz":2.47e-8
    },
    "Materials":
    {
        "Cu":
        {
            "k":"sigma0*Lorentz*heat_T/(1+alpha*(heat_T-T0)):sigma0:alpha:T0:Lorentz:heat_T",
            //"Cp":"1000.005", //[ J/(kg*K) ]
            // "rho":"1.205", //[ kg/(m^3) ]
            "sigma":"sigma0/(1+alpha*(heat_T-T0))+0*heat_T:sigma0:alpha:T0:heat_T"
        }
    },
    "BoundaryConditions":
    {
        "temperature":
        {
            "Robin":
            {
                "Rint":
                {
                    "expr1":"0.085", // h
                    "expr2":"290" // T_c
                },
                "Rext":
                {
                    "expr1":"0.085", // h
                    "expr2":"290" // T_c
                }
            },
            "Neumann":
            {
                "Interface":
                {
                    "expr":"0"
                },
                "GR_1_Interface":
                {
                    "expr":"0"
                }
            }
        },
        "electric-potential":
        {
            "Dirichlet":
            {
                "V0":
                {
                    "expr":"0" // V_in
                },
                "V1":
                {
                    "expr":"9" // V_out
                }
            }
        }
    },
    "PostProcess":
    {
        "use-model-name":1,
        "thermo-electric":
        {
            "Exports":
            {
                "fields":["heat.temperature","electric.electric-potential","electric.electric-field","electric.current-density","heat.pid"]
            }
        },
        "electric":
        {
            "Measures":
            {
                "Statistics":
                {
                    "Intensity0":
                    {
                        "type":"integrate",
                        "expr":"-electric_sigma_Cu*electric_dnP:electric_sigma_Cu:electric_dnP",
                        "markers":"V0"
                    },
                    "Intensity1":
                    {
                        "type":"integrate",
                        "expr":"-electric_sigma_Cu*electric_dnP:electric_sigma_Cu:electric_dnP",
                        "markers":"V1"
                    }
                },
                "Points":
                {
                    "closeToV0":
                    {
                        "coord":"{0.6,22,108}",
                        "fields":"electric-potential"
                    },
                    "closeToV1":
                    {
                        "coord":"{0.6,22,-226}",
                        "fields":"electric-potential"
                    },
                    "middle":
                    {
                        "coord":"{0.6,22,0}",
                        "fields":["electric-potential","electric-field" ]
                    }
                }
            },
            "Checkers":
            {
                "Measures":
                {
                    "Statistics_Intensity0_integrate":
                    {
                        "value":2.9486355582068285e+04,
                        "tolerance":1e-1
                    },
                    "Statistics_Intensity1_integrate":
                    {
                        "value":-2.9492993990629227e+04,
                        "tolerance":1e-1
                    },
                    "Points_closeToV0_electric-potential":
                    {
                        "value":0,
                        "tolerance":1e-3
                    },
                    "Points_closeToV1_electric-potential":
                    {
                        "value":9,
                        "tolerance":1e-3
                    }
                }
            }

        }

    }
}
