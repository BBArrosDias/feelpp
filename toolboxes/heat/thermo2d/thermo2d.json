// -*- mode: javascript -*-
{
    "Name": "Test heat toolbox",
    "ShortName":"TestHeat",
    // "Parameters":
    // {
    //     "gravity":
    //     {
    //         "value":"-1"
    //     }
    // },
    "Materials":
    {
        "mymat1":
        {
            "markers":"Omega1",
            "k":"10",//[ W/(m*K) ]
            //"k":"10+100*(1+y):y",//[ W/(m*K) ]
            //"k":"{10,0,0,100}",//[ W/(m*K) ]
            //"k":"10*heat_T/293:heat_T",//[ W/(m*K) ]
            "Cp":"1000.005", //[ J/(kg*K) ]
            "rho":"1.205" //[ kg/(m^3) ]
        },
        "mymat2":
        {
            "markers":"Omega2",
            "k":"100",
            "Cp":"3500",
            "rho":"2"
        }
    },
    "BoundaryConditions":
    {
        "temperature":
        {
            "Dirichlet":
            {
                "MarkerDirichlet":
                {
                    "expr":"293-10*(y-1)*(y+1):y"
                },
                "wall":
                {
                    "expr":"293+0.38*t:t"
                }
            },
            "Neumann":
            {
                "MarkerNeumann":
                {
                    "expr":"0:x" 
                }
            },
            "Robin":
            {
                "MarkerRobin":
                {
                    "expr1":"16.667",// h coeff
                    "expr2":"287"// temperature exterior
                }
            }
        }
    },
    "InitialConditions":
    {
        "temperature":
        {
            "Expression":
            {
                "myic1":
                {
                    "markers":"Omega1",
                    "expr":"293"
                },
                "myic2":
                {
                    "markers":"Omega2",
                    "expr":"305"
                },
                "myic3":
                {
                    "markers":"Interface_1_2",
                    "expr":"310"
                }
            }/*,
            "File":
            {
                "myic3":
                {
                    "filename":"$home/feel/toolboxes/heat/thermo2d/P1G1/np_1/heat.ts/temperature/temperature-199.h5",
                    "format":"hdf5"
                    //"directory":"$home/feel/toolboxes/heat/thermo2d/P1G1/np_1/heat.ts/temperature"
                }
            }*/
        }
    },
    "PostProcess":
    {
        "Exports":
        {
            "fields":["temperature"]
        },
        "Measures":
        {
            "Normal-Heat-Flux":["MarkerRobin","MarkerNeumann"],
            "Points":
            {
                "ptA":
                {
                    "coord":"{-0.6,0.6}",
                    "fields":"temperature"
                }
            }
        }
    }

}
